generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["dev"]
}

model User {
  id       String @id @default(uuid())
  email    String @unique
  name     String
  password String
  role     String

  @@schema("dev")
}
